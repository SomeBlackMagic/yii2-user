{
    "name": "someblackmagic/yii2-user",
    "description": "Flexible user registration and authentication module for Yii2",
    "keywords": ["yii2", "yii2-user", "user management", "dektrium", "someblackmagic"],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Erofeev",
            "email": "dmeroff@gmail.com",
            "homepage": "http://dmeroff.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "yiisoft/yii2": "^2.0.13",
        "yiisoft/yii2-swiftmailer": "^2.0.0",
        "yiisoft/yii2-authclient": "^2.0.0",
        "yiisoft/yii2-bootstrap": "^2.0.0"
    },
    "require-dev": {
        "codeception/codeception": "^2.5",
        "codeception/specify": "^0.4.3",
        "codeception/verify": "^0.3.1",
        "yiisoft/yii2-codeception": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "SomeBlackMagic\\Yii2User\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
